{
	"compilerOptions": {
		"target": "ES2020",
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "node",
		"declaration": true,
		"outDir": "./lib",
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}
